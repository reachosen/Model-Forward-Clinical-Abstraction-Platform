{
  "data_schemas": {
    "patient_case": {
      "type": "object",
      "description": "Complete patient case data for CAUTI evaluation",
      "properties": {
        "patient_id": {"type": "string"},
        "encounter_id": {"type": "string"},
        "devices": {
          "catheter_insertion_date": {"type": "string", "format": "date"},
          "catheter_type": {"type": "string", "enum": ["foley", "suprapubic", "nephrostomy"]},
          "catheter_removal_date": {"type": "string", "format": "date"}
        },
        "lab_results": {
          "type": "array",
          "items": {"$ref": "#/definitions/UrineLabResult"}
        },
        "clinical_signs": {
          "fever": {"type": "boolean"},
          "dysuria": {"type": "boolean"},
          "urgency": {"type": "boolean"},
          "suprapubic_tenderness": {"type": "boolean"}
        }
      }
    },
    "definitions": {
      "UrineLabResult": {
        "type": "object",
        "properties": {
          "collection_date": {"type": "string", "format": "date"},
          "organism": {"type": "string"},
          "cfu_count": {"type": "number"},
          "sample_method": {"type": "string", "enum": ["catheter", "suprapubic", "clean_catch"]}
        }
      }
    }
  }
}
