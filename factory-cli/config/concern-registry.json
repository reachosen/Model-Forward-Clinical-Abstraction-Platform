    "H17_neurosurgical_readmissions": {
      "pattern": "^H17[abcde]$",
      "domain": "Neurology",
      "archetype": "Preventability_Detective",
      "description": "Neurosurgical readmissions"
    },
    "H_neurosurgical_return_to_or": {
      "pattern": "^(H17\\.1|H17|H29|H34)$",
      "domain": "Neurology",
      "archetype": "Preventability_Detective",
      "description": "Neurosurgical unplanned return to OR"
    },
    "H31_epilepsy_seizure_freedom": {
      "pattern": "^H31\\.[1234]$",
      "domain": "Neurology",
      "archetype": "Process_Auditor",
