{
  "name": "clinical-planner-cli",
  "version": "2.0.0",
  "description": "Clinical Abstraction Planner CLI - Multi-domain surveillance configuration generator for HAC, USNWR, Quality, and Safety metrics",
  "main": "cli/plan.ts",
  "scripts": {
    "planner": "ts-node bin/planner.ts",
    "rpi": "ts-node bin/planner.ts rpi",
    "rpi:clabsi": "ts-node bin/planner.ts rpi --concern CLABSI --domain picu",
    "rpi:cauti": "ts-node bin/planner.ts rpi --concern CAUTI --domain picu",
    "research": "ts-node bin/planner.ts research",
    "cache:list": "ts-node bin/planner.ts cache list",
    "cache:clear": "ts-node bin/planner.ts cache clear",
    "generate": "ts-node bin/planner.ts generate",
    "generate:clabsi": "ts-node bin/planner.ts generate --concern CLABSI --domain picu",
    "plan": "ts-node cli/plan.ts",
    "plan:clabsi": "ts-node cli/plan.ts examples/clabsi_planning_input.json",
    "plan:vap": "ts-node cli/plan.ts examples/vap_planning_input.json",
    "validate:clabsi": "ts-node cli/plan.ts examples/clabsi_planning_input.json --validate-only",
    "validate:vap": "ts-node cli/plan.ts examples/vap_planning_input.json --validate-only",
    "learn": "ts-node cli/learn.ts",
    "learn:status": "ts-node cli/learn.ts --status",
    "learn:mock": "ts-node cli/learn.ts --mock",
    "revise": "ts-node cli/revise.ts",
    "batch": "ts-node orchestrator/batch-runner.ts",
    "test:gates": "npm run test:semantic && ts-node orchestrator/test-gates-i32b.ts",
    "test:full": "npm run test:semantic && ts-node orchestrator/test-s0-s6-complete.ts",
    "test:s6": "ts-node tests/integration/s6-sufficiency.test.ts",
    "flywheel": "ts-node bin/planner.ts flywheel",
    "test": "ts-node tests/integration/rpi-workflow.test.ts && ts-node tests/integration/s6-sufficiency.test.ts",
    "test:integration": "ts-node tests/integration/rpi-workflow.test.ts",
    "build": "tsc",
    "clean": "rm -rf dist *.json validation_report.txt",
    "missions": "ts-node tools/mission-control.ts help",
    "missions:list": "ts-node tools/mission-control.ts list",
    "missions:run": "ts-node tools/mission-control.ts run",
    "missions:pick": "ts-node tools/mission-control.ts pick",
    "test:semantic": "ts-node tests/semantic/semantic-contracts.test.ts"
  },
  "keywords": [
    "healthcare",
    "clinical-abstraction",
    "surveillance",
    "HAC",
    "hospital-acquired-conditions",
    "CLABSI",
    "CAUTI",
    "VAP",
    "SSI",
    "USNWR",
    "quality-metrics",
    "clinical-informatics",
    "NHSN",
    "pediatrics",
    "orthopedics",
    "endocrinology"
  ],
  "author": "Clinical Informatics Team",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "commander": "^11.1.0",
    "dotenv": "^17.2.3"
  },
  "optionalDependencies": {
    "openai": "^4.24.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
